<div class="row">
  <div class="col-xs-8 col-xs-offset-2">
    <% if @model == 'user' %>
      <!--users一覧 -->
      <h2>Users search for '<%= @content %>'</h2>
      <table class="table">
        <thead>
          <tr>
            <th>icon</th>
            <th>name</th>
            <th colspan="3"></th>
          </tr>
        </thead>
        <tbody>
          <% @datas.each do |f| %>
            <tr>
              <td><%= attachment_image_tag(f, :user_image, size: "50x50", fallback: f.default_image) %></td>
              <td><%= f.name %></td>
              <td><%= link_to "詳細へ", users_user_path(f.id) %></td>
            </tr>
          <% end %>
        </tbody>
      </table>
    <% else %>
      <!--items一覧 -->
      <h2>Images search for '<%= @content %>'</h2>
      <table class="table">
        <thead>
          <tr>
            <th>title</th>
            <th>Opinion</th>
            <th colspan="3"></th>
          </tr>
        </thead>
        <tbody>
          <% @datas.each do |f| %>
            <tr>
              <td><%= f.title %></td>
              <td><%= f.description %></td>
              <td><%= link_to "詳細へ", users_item_path(f.id) %></td>
            </tr>
          <% end %>
        </tbody>
      </table>
    <% end %>
  </div>
  <div class="space"></div>
  <div class="space3"></div>
</div>